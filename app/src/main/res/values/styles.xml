<resources>

    <!-- Base application theme. -->
    <style name="AppTheme" parent="Theme.AppCompat.Light.DarkActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/colorPrimary</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDark</item>
        <item name="colorAccent">@color/colorAccent</item>
    </style>

    <style name="GHSample">
    </style>

    <style name="GHSample.Title">
        <item name="android:textSize">@dimen/textTitle</item>
        <item name="android:textColor">@color/textPrimary</item>
    </style>

    <style name="GHSample.Description">
        <item name="android:textSize">@dimen/textDescription</item>
        <item name="android:textColor">@color/textSecondary</item>
    </style>

    <style name="GHSample.Attributes">
        <item name="android:textSize">@dimen/textDescription</item>
        <item name="android:textColor">@color/textSecondary</item>
    </style>

</resources>
